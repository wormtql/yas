    "version": "0.2.0",
    "configurations": [
           {
    "name": "Debug",
    "type": "gdb",  // 使用Native Debug插件提供的gdb类型
    "request": "launch",
    "target": "./target/debug/${workspaceFolderBasename}.exe", // 注意这里使用了第五部分讲到的配置变量
